<div class="container text-center">
  <h1>You've made a new Mooking!</h1>
  <ul>
    <div class="card">
      <p>Your Cow is <%= @booking.cow.name %> 🐄</p>
      <p>From <%= @booking.start_date %></p>
      <p>until <%= @booking.end_date %></p>
      <p>Owner contact details: <%= @booking.cow.user.email %></p>
    </div>
  </ul>
  <%= link_to "Back to all cows",  cows_path, class: "btn btn-flat" %>
</div>
